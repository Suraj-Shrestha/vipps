{
    "name": "zaporylie/vipps",
    "description": "Vipps by DNB PHP SDK",
    "type": "library",
    "keywords": ["payment"],
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/http-message": "^1.0",
        "php-http/message": "^1.6",
        "php-http/httplug": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.0",
        "eloquent/enumeration": "^5.1",
        "jms/serializer": "^1.8"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": ">=5 <8",
        "symfony/yaml": "^2.0",
        "symfony/filesystem": "^3.1",
        "squizlabs/php_codesniffer": "^2.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "suggest": {
        "php-http/guzzle6-adapter": "Guzzle adapter"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Piasecki",
            "email": "jakub@piaseccy.pl"
        }
    ],
    "scripts": {
        "test": [
            "composer test-style",
            "composer test-unit"
        ],
        "test-style": "./vendor/bin/phpcs -p",
        "fix-style": "./vendor/bin/phpcbf -p",
        "test-unit": "./vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": { "zaporylie\\Vipps\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "zaporylie\\Vipps\\Tests\\": "test/" }
    }
}
